@{
	ViewData["Title"] = "Excel Reader";
}
<div class="text-center">
	<form method="post" >
		<div class="active-cyan-4 mb-4">
			<div>
				<input class="form-control" type="text" placeholder="Enter Subject ID" name="Subject">
				<div class="col-1">
					<input type="submit" class="btn btn-success" value="Choose this subject" asp-controller="Timetable" asp-action="SubjectSearch" />
				</div>
			</div>
		</div>
	</form>
	<div class="active-cyan-4 mb-4">
		<div>
			<input class="form-control" type="text" placeholder="Enter Subject ID" id="Subject">
			<div class="col-1">
				<button class="btn btn-success" onclick="subjectSearch()">Find this subject</button>
			</div>
		</div>
	</div>
</div>
<script src="~/js/SubjectSearch.js" asp-append-version="true"></script>

